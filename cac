                ['SeaDragon'] = false, --// Abyssal Tyrant
                ['ThirdSeaDragon'] = false, --// Drakenfyr the Inferno King
                ['FuryTentacle'] = false, --// Chaos Kraken
            },--// If true, it will skip and continue hopping.
            ['Sea Map'] = 'ThirdSea', --// ThirdSea, SecondSea, FirstSea - It will automatically return to the selected sea and hop servers.
