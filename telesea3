local function teleportToThirdSea()
    local replicatedStorage = game:GetService("ReplicatedStorage")
    local remotes = replicatedStorage:FindFirstChild("Remotes")
    local commF = remotes and remotes:FindFirstChild("CommF_")

    if not commF then
        warn("Không tìm thấy CommF_! Đảm bảo bạn đang sử dụng đúng script.")
        return
    end

    if game.PlaceId == 4442272183 then -- Nếu đang ở Second Sea
        print("Đang thực hiện teleport đến Third Sea...")
        local success, err = pcall(function()
            commF:InvokeServer("TravelZou")
        end)
        if success then
            print("Teleport thành công đến Third Sea!")
        else
            warn("Lỗi khi teleport đến Third Sea:", err)
        end
    else
        print("Bạn không ở Second Sea hoặc đã ở Third Sea.")
    end
end

-- Gọi hàm tự động khi script được load
teleportToThirdSea()
